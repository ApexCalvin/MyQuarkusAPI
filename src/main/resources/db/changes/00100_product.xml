<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:pro="http://www.liquibase.org/xml/ns/pro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet author="trax" id="GATE">
        <createTable tableName="GATE">
            <column name="ID" type="VARCHAR2(40 CHAR)" defaultValueComputed="SYS_GUID()" remarks="Unique ID auto generated">
                <constraints primaryKey="true" nullable="false" primaryKeyName="GATE_PK"/>
            </column>
            <column name="LOCATION" type="VARCHAR2(10 CHAR)" remarks="The location identifier">
                <constraints nullable="false"/>
            </column>
            <column name="GATE" type="VARCHAR2(20 CHAR)" remarks="The gate identifier">
                <constraints nullable="false"/>
            </column>
            <column name="STATUS" type="VARCHAR2(10 CHAR)" defaultValue="ACTIVE"/>
            <column name="CREATED_BY" type="VARCHAR2(10 CHAR)" defaultValue="SYSTEM"/>
            <column name="CREATED_DATE" type="date" defaultValueDate="SYSDATE"/>
            <column name="MODIFIED_BY" type="VARCHAR2(10 CHAR)"/>
            <column name="MODIFIED_DATE" type="date" defaultValueDate="SYSDATE"/>
        </createTable>
        <!-- Unique constraint for business rule enforcement -->
        <addUniqueConstraint tableName="GATE" columnNames="LOCATION, GATE" constraintName="UNIQUE_LOCATION_GATE" initiallyDeferred="false" validate="true"/>
    </changeSet>
        <changeSet author="trax" id="AC_MASTER_LOAD">
        <loadData
            commentLineStartsWith="#"
            encoding="UTF-8"
            file="ac_master.csv"
            relativeToChangelogFile="true"
            tableName="AC_MASTER"
            quotchar="&quot;" separator=","
            usePreparedStatements="true">
            <column header="AC" name="AC" type="STRING" />
            <column header="AC_TYPE" name="AC_TYPE" type="STRING" />
            <column header="AC_SERIES" name="AC_SERIES"
                type="STRING" />
            <column header="AC_DESCRIPTION" name="AC_DESCRIPTION" type="STRING" />
            <column header="AC_SN" name="AC_SN" type="STRING" />
            <column
                header="CUSTOMER" name="CUSTOMER" type="STRING" />
            <column header="MANUFACTURE" name="MANUFACTURE" type="STRING" />
            <column header="OPERATOR"
                name="OPERATOR" type="STRING" />
            <column header="STATUS" name="STATUS" type="STRING" />
            <column header="ETOPS" name="ETOPS" type="STRING" />
            <column
                header="ETOPS_MINUTES" name="ETOPS_MINUTES" type="STRING" />
            <column header="ETOPS_DOWNGRADE" name="ETOPS_DOWNGRADE" type="STRING" />
        </loadData>
    </changeSet>
</databaseChangeLog>